{
  "name": "Moodle Logger",
  "version": "1.0",
  "description": "login to moodle automaticaly!",
  "content_scripts": [
    {
      "matches": ["*://*.tau.ac.il/*"],
      "js": ["sendToLogin.js"]
    },
    {
      "matches": ["*://moodle.tau.ac.il/*"],
      "js": ["login.js"]
    }
  ],
  "permissions": ["storage"],
  "manifest_version": 2
}
